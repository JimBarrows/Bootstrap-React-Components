{
  "name": "bootstrap-react-components",
  "version": "0.17.1",
  "description": "Bootstrap components for react.",
  "main": "./distribution/index.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir distribution",
    "build:docker": "cd features/support;docker build --tag bootstrap-react-components-test --rm .",
    "dev:src": "watch 'babel src --presets babel-preset-es2015 --out-dir features/support/html/js' ./src",
    "dev:harness": "webpack --progress --colors --watch -d --config features/support/webpack.config.js",
    "dev:host:start": "docker run --rm -p 80:80 --name bootstrap-react-components-test-host -d -v $PWD/features/support/html:/usr/share/nginx/html bootstrap-react-components-test",
    "dev:host:stop": "docker stop bootstrap-react-components-test-host",
    "prepublish": "npm run build",
    "test:host:start": "docker run --rm -p 80:80 --name bootstrap-react-components-test-host -d -v $PWD/features/support/html:/usr/share/nginx/html bootstrap-react-components-test",
    "test:host:stop": "docker stop bootstrap-react-components-test-host",
    "test:src": "babel src --presets babel-preset-es2015 --out-dir features/support/html/js",
    "test:harness": "webpack --progress --colors -d --config features/support/webpack.config.js",
    "test:behavior": "cucumber-js  --compiler es6:babel-core/register",
    "test": "npm run test:host:start; npm run test:src; npm run test:harness; npm run test:behavior; npm run test:host:stop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimBarrows/Bootstrap-React-Components.git"
  },
  "author": {
    "name": "Jim Barrows",
    "email": "Jim.Barrows@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/JimBarrows/Bootstrap-React-Components/issues"
  },
  "homepage": "https://github.com/JimBarrows/Bootstrap-React-Components#readme",
  "peerDependencies": {},
  "dependencies": {
    "bootstrap": "^3.3.7",
    "css-loader": "^0.25.0",
    "draft-js": "^0.9.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",
    "moment": "^2.14.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-rte": "^0.11.0",
    "react-datetime": "^2.6.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "chai": "^4.0.2",
    "chai-as-promised": "^7.0.0",
    "chai-things": "^0.2.0",
    "cucumber": "^2.3.1",
    "eslint": "^4.0.0",
    "eslint-plugin-babel": "^4.1.1",
    "style-loader": "^0.13.2",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
