{
  "name": "bootstrap-react-components",
  "version": "1.1.2",
  "description": "Bootstrap components for react.",
  "main": "./distribution/index.js",
  "scripts": {
    "build": "babel src  --out-dir distribution",
    "build:docker": "cd features/support;docker build --tag bootstrap-react-components-test --rm .",
    "dev:src": "watch 'babel src --out-dir features/support/html/js' ./src",
    "dev:harness": "webpack --progress --colors --watch -d --config features/support/webpack.config.js",
    "dev:host:start": "docker run --rm -p 80:80 --name bootstrap-react-components-test-host -d -v $PWD/features/support/html:/usr/share/nginx/html bootstrap-react-components-test",
    "dev:host:stop": "docker stop bootstrap-react-components-test-host",
    "prepublish": "npm run build",
    "test:host:start": "docker run --rm -p 80:80 --name bootstrap-react-components-test-host -d -v $PWD/features/support/html:/usr/share/nginx/html bootstrap-react-components-test",
    "test:host:stop": "docker stop bootstrap-react-components-test-host",
    "test:src": "babel src --out-dir features/support/html/js",
    "test:harness": "webpack --progress --colors -d --config features/support/webpack.config.js",
    "test:behavior": "cucumber-js  --compiler es6:babel-core/register",
    "test": "npm run test:host:start; npm run test:src; npm run test:harness; npm run test:behavior; npm run test:host:stop",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JimBarrows/Bootstrap-React-Components.git"
  },
  "author": {
    "name": "Jim Barrows",
    "email": "Jim.Barrows@gmail.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/JimBarrows/Bootstrap-React-Components/issues"
  },
  "homepage": "https://github.com/JimBarrows/Bootstrap-React-Components#readme",
  "peerDependencies": {
    "react": "16.x.x",
    "react-dom": "16.x.x"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "~1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "bootstrap": "^4.5.0",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "moment": "^2.25.3",
    "prop-types": "^15.7.2",
    "react-datetime": "^2.16.3",
    "react-quill": "^1.3.5",
    "url-loader": "^4.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.9.0",
    "@babel/plugin-transform-react-inline-elements": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.6",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-react-html-attrs": "^3.0.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "cucumber": "^6.0.5",
    "eslint": "^7.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-standard": "^4.0.1",
    "popper.js": "^1.14.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-dev-server": "^3.11.0"
  }
}
