{
  "extends": [
    "standard",
    "standard-react"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "indent": [
      "error",
      2,
      {
        "FunctionDeclaration": {
          "parameters": "first"
        },
        "FunctionExpression": {
          "parameters": "first"
        },
        "CallExpression": {
          "arguments": "first"
        },
        "ObjectExpression": 1
      }
    ]
  }
}